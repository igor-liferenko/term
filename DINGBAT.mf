x4=x5=good.x(9/10uniform_square);
 
y1=y2=good.y(1/10uniform_square);
y6=y7=good.y(4/10uniform_square);
y3=y4=good.y(8/10uniform_square);
y5=y8=good.y(uniform_square);
 
pickup chunkypen;
